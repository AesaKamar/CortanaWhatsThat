<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us">
    <AppName>Identify</AppName>
    <Example>Identify 'this' or 'that'</Example>
    <Command Name="Identify">
      <Example>Identify that</Example>

      <ListenFor RequireAppName="BeforePhrase">this</ListenFor>
      <ListenFor RequireAppName="BeforePhrase">that</ListenFor>
      <Feedback> Ok </Feedback>
      <VoiceCommandService Target="VoiceCommandService"/>
    </Command>
  </CommandSet>
</VoiceCommands>